    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "nouislider": "14.2.0",
    "perfect-scrollbar": "1.5.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-image-gallery": "^1.0.7",
    "react-pdf": "^5.0.0-beta.3",
    "react-router-dom": "5.1.2",
